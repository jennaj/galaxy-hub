            div.col-md-9
                section.section-content
                    +renderPageTitle
                    div !{ contents }
            div.col
                div#sidebar
                    +renderToc(toc)
        else
            div.col-md-12
                section.section-content
                    +renderPageTitle
                    div !{ contents }
